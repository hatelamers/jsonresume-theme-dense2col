{{#if resume.projects.length}}
<section id="projects">
	<h2 class="xfa-icon">{{label-lookup "Projects"}}</h2>
	{{#each resume.projects}}
	<div class="item">
		{{#if (or name entity)}}
        <div class="item-head">
			<h3 class="name part">{{name}}</h3>
			<div class="entity part">{{entity}}</div>
        </div>
		{{/if}}
		{{#if (or roles.length date)}}
        <div class="item-head">
			<div class="position part">{{#if roles.length}}{{#each roles}}{{#unless @first}}, {{/unless}}<span>{{.}}</span>{{/each}}{{/if}}</div>
			<div class="date part">{{> datespan}}</div>
        </div>
		{{/if}}
		{{#if url}}
 		<div class="url">
			<a href="{{url}}">{{url}}</a>
		</div>
		{{/if}}
		{{#if description}}
		<div class="description">
			<p>{{description}}</p>
		</div>
		{{/if}}
		{{#if highlights.length}}
		<ul class="highlights">
			{{#each highlights}}
			<li>{{.}}</li>
			{{/each}}
		</ul>
		{{/if}}
		{{#if keywords.length}}
		    <ul class="keywords">
			      {{#each keywords}}
			          <li>{{.}}</li>
			      {{/each}}
		    </ul>
		{{/if}}
	</div>
	{{/each}}
</section>
{{/if}}
